{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "ordered-imports": [
            true,
            {
              "import-sources-order": "any",
              "named-imports-order": "any"
            }
          ],
          "quotemark": [true, "single"],
          "no-console": [false, "log", "error"]
    },
    "rulesDirectory": []
}